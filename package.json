    "url": "https://github.com/rpt24saltysnack/somebirds-product-images/issues"
  },
  "homepage": "https://github.com/rpt24saltysnack/somebirds-product-images#readme",
  "dependencies": {
    "aws-sdk": "^2.821.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.14",
    "mysql2": "^2.2.5",
    "newrelic": "^7.1.1",
    "node-fetch": "^2.6.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-modal": "^3.12.1",
    "unsplash-js": "^7.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.2",
    "cloudinary": "^1.23.0",
    "css-loader": "^5.0.1",
    "csvtojson": "^2.0.10",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.6",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0"
  }
